{
  "name": "preact-redis-demo",
  "version": "1.0.0",
  "description": "Componente que entrega el tiempo y hora de una ciudad, Preact+Redis+Typescript en client + Node expressAPI server",
  "main": "index.js",
  "dependencies": {
    "axios": "^0.16.1",
    "moment-timezone": "^0.5.13",
    "preact": "^8.1.0",
    "ts-loader": "^2.0.3",
    "typescript": "^2.3.2",
    "webpack": "^2.5.0"
  },
  "devDependencies": {
    "@types/chai": "^4.0.5",
    "@types/mocha": "^2.2.44",
    "@types/sinon": "^4.0.0",
    "chai": "^4.1.2",
    "mocha": "^4.0.1",
    "sinon": "^4.1.2",
    "ts-node": "^3.3.0"
  },
  "scripts": {
    "install": "npm i && cd src/api && npm i",
    "build": "tsc && ./node_modules/.bin/webpack",
    "build-server": "cd src/api && npm run build",
    "start": "node src/api/src/index.js",
    "postinstall": "./node_modules/.bin/webpack",
    "test": "mocha --compilers ts:ts-node/register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://marioaraya@github.com/MarioAraya/preact-redis-demo.git"
  },
  "keywords": [
    "preact",
    "redis",
    "typescript",
    "aws"
  ],
  "author": "mario.araya.romero@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MarioAraya/preact-redis-demo/issues"
  },
  "homepage": "https://github.com/MarioAraya/preact-redis-demo#readme",
  "engines": {
    "node": "6.9.1"
  }
}
